  "scripts": {
    "jss": "jss",
    "deploy-app": "jss deploy app -d --acceptCertificate F0:79:6E:FD:7F:47:7B:EF:03:81:29:41:D3:B7:CA:F8:97:23:A4:2E",
    "start": "npm-run-all --serial bootstrap:disconnected --parallel start:react start:proxy start:watch-components watch-css",
    "start:connected": "scjss-verify-setup && npm-run-all --serial bootstrap:connected start:react start:watch-components watch-css",
    "start:connected-ssr": "scjss-verify-setup && npm-run-all --serial bootstrap:connected --parallel start:react start:watch-components start:http-renderer watch-css",
    "start:holygrail": "scjss-verify-setup && npm-run-all --serial bootstrap:connected --parallel start:watch-components start:http-renderer-holygrail watch-css",
    "start:http-renderer-holygrail": "cross-env-shell NODE_ENV=development JSS_RENDER_ENGINE=http \"node scripts/holygrail.js\"",
    "start:rendering-host": "cross-env-shell NODE_ENV=development JSS_RENDER_ENGINE=http \"node scripts/http-renderer.js\"",
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:disconnected": "node scripts/bootstrap.js --disconnected",
    "build": "npm-run-all --serial bootstrap:connected build:client build:server build-css",
    "build:rendering-host": "npm-run-all --serial bootstrap:connected build:client:rendering-host build:server",
    "build-storybook": "build-storybook -s public",
    "scaffold": "node scripts/scaffold-component.js",
    "start:react": "craco start",
    "start:proxy": "node scripts/disconnected-mode-proxy.js",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "build:client": "cross-env-shell PUBLIC_URL=$npm_package_config_sitecoreDistPath \"craco build\"",
    "build:client:rendering-host": "cross-env-shell PUBLIC_URL=$npm_package_config_tunnelUrl \"craco build\"",
    "build:server": "cross-env-shell NODE_ENV=production \"webpack --config server/server.webpack.config.js\"",
    "build-css": "postcss src/css/app.css -o public/app.css",
    "watch-css": "postcss src/css/app.css -o public/app.css --watch --recursive",
    "graphql:update": "node -r babel-register ./scripts/update-graphql-fragment-data.js",
    "test": "react-scripts test --env=jsdom",
    "storybook": "start-storybook -p 9009 -s public"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint",
      "pretty-quick â€” staged",
      "git add"
    ]
  }
}
